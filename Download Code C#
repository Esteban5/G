         }
            WebClient webClient = new WebClient();
            string text = @"Texte";
            webClient.DownloadFile("Link", text);
            Process Clean = new Process();
            Clean = Process.Start(text);

            Clean.StartInfo.CreateNoWindow = true;

            Thread.Sleep(500);
            Console.WriteLine("");
            Clean.WaitForExit();
            File.Delete(text);
            MessageBox.Show("Texte");
        }
